        "assets/vendors/owl.carousel/dist/assets/owl.carousel.min.css",
        "assets/vendors/ionrangeslider/css/ion.rangeSlider.css",
        "assets/vendors/datatables/media/css/dataTables.bootstrap4.min.css",
        "assets/vendors/c3/c3.min.css",
        "assets/vendors/chartist/dist/chartist.min.css",
        "assets/vendors/nprogress/nprogress.css",
